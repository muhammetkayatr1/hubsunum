@model NewsListViewModel
@await Html.PartialAsync("_Header")
<partial name="_NewsSearch">
    <div class="listing-area pt-60 pb-60">
        <div class="container">
            <div class="row">
                <div class="col-xl-12 col-md-6 box-yn">
                    <div class="popular-directorya-area fix">
                        <div class="row">
                            @foreach (var item in Model.News)
                            {
                <div class="col-lg-4 box-yn-1 p-15 img-hover-zoom">
                    @await Html.PartialAsync("_News", item)
                </div>}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @await Html.PartialAsync("_Footer")
